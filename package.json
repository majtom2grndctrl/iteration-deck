{
  "name": "iteration-deck",
  "version": "0.0.1",
  "description": "AI-first prototyping workflow components for comparing UI variations",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/iteration-deck/iteration-deck.esm.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs.js",
      "types": "./dist/types/index.d.ts"
    },
    "./components": {
      "import": "./dist/components/index.js",
      "types": "./dist/components/index.d.ts"
    },
    "./components/iteration-deck": {
      "import": "./dist/components/iteration-deck.js",
      "types": "./dist/components/iteration-deck.d.ts"
    },
    "./components/iteration-deck-slide": {
      "import": "./dist/components/iteration-deck-slide.js",
      "types": "./dist/components/iteration-deck-slide.d.ts"
    },
    "./components/iteration-deck-toolbar": {
      "import": "./dist/components/iteration-deck-toolbar.js",
      "types": "./dist/components/iteration-deck-toolbar.d.ts"
    },
    "./dist": {
      "import": "./dist/iteration-deck/iteration-deck.esm.js",
      "require": "./dist/cjs/iteration-deck.cjs.js",
      "types": "./dist/types/index.d.ts"
    },
    "./loader": {
      "import": "./loader/index.js",
      "require": "./loader/index.cjs.js",
      "types": "./loader/index.d.ts"
    },
    "./react": {
      "import": "./dist/collection/react/index.js",
      "types": "./dist/types/react/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dhiester/iteration-deck.git"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "typecheck": "tsc --noEmit",
    "lint": "echo 'Linting not configured yet'",
    "generate": "stencil generate"
  },
  "devDependencies": {
    "@stencil/core": "^4.27.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.5",
    "@types/react": "^19.1.10",
    "@vanilla-extract/rollup-plugin": "^1.4.1",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "puppeteer": "^24.3.0",
    "react": "^19.1.1",
    "typescript": "^5.9.2"
  },
  "license": "MIT",
  "dependencies": {
    "@vanilla-extract/css": "^1.17.4"
  }
}
