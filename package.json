{
  "name": "iteration-deck",
  "version": "0.1.0",
  "description": "React components for AI-first prototyping workflows that enable designers and PMs to compare AI-generated UI variations interactively",
  "type": "module",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.js",
  "es2015": "./dist/esm/index.mjs",
  "es2017": "./dist/esm/index.mjs",
  "types": "./dist/types/index.d.ts",
  "collection": "./dist/collection/collection-manifest.json",
  "collection:main": "./dist/collection/index.js",
  "unpkg": "./dist/iteration-deck/iteration-deck.esm.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs.js",
      "types": "./dist/types/index.d.ts"
    },
    "./loader": {
      "import": "./dist/loader/index.js",
      "require": "./dist/loader/index.cjs.js",
      "types": "./dist/types/loader/index.d.ts"
    },
    "./react": {
      "import": "./dist/generated/react/components.js",
      "types": "./dist/generated/react/components.d.ts"
    },
    "./styles": "./dist/iteration-deck/iteration-deck.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "stencil build --dev --watch --serve",
    "build": "stencil build",
    "build.watch": "stencil build --watch",
    "build.prod": "stencil build --prod",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec",
    "test.watch": "stencil test --spec --watchAll",
    "test.e2e": "stencil test --e2e",
    "generate": "stencil generate",
    "lint": "eslint src/**/*.ts",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm run build.prod"
  },
  "keywords": [
    "react",
    "typescript",
    "ai",
    "prototyping",
    "design",
    "components",
    "ui",
    "variations",
    "iteration",
    "ai-first",
    "design-tools",
    "ui-variations",
    "component-comparison",
    "design-iteration",
    "ai-generated-ui",
    "prototype-comparison",
    "design-system",
    "ui-prototyping",
    "variation-testing",
    "ab-testing",
    "ui-testing",
    "design-workflow",
    "ai-coding-agent",
    "ai-assistant",
    "claude",
    "chatgpt",
    "cursor",
    "copilot",
    "web-components",
    "custom-elements",
    "lit",
    "universal",
    "framework-agnostic",
    "astro"
  ],
  "author": "Your Name",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/iteration-deck.git"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "dependencies": {
    "lit": "^3.3.1",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@stencil/core": "^4.36.2",
    "@stencil/react-output-target": "^1.2.0",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vanilla-extract/css": "^1.17.4",
    "@vanilla-extract/dynamic": "^2.1.5",
    "@vanilla-extract/rollup-plugin": "^1.4.1",
    "@vanilla-extract/vite-plugin": "^5.1.1",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "jsdom": "^26.1.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "rollup-plugin-postcss": "^4.0.2",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1"
  }
}
