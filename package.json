{
  "name": "iteration-deck",
  "version": "0.0.0",
  "description": "AI-first prototyping web components for comparing interactive UI variations. Perfect for AI coding agents to create multiple design variations and A/B test prototypes.",
  "homepage": "https://github.com/MajTom2GrndCtrl/iteration-deck",
  "repository": {
    "type": "git",
    "url": "https://github.com/MajTom2GrndCtrl/iteration-deck.git"
  },
  "bugs": {
    "url": "https://github.com/MajTom2GrndCtrl/iteration-deck/issues"
  },
  "author": {
    "name": "Dan Hiester"
  },
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "main": "./dist/core.cjs",
  "module": "./dist/core.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/core.js",
      "require": "./dist/core.cjs"
    },
    "./react": {
      "types": "./dist/react/index.d.ts",
      "import": "./dist/react.js",
      "require": "./dist/react.cjs"
    },
    "./styles": "./dist/style.css",
    "./package.json": "./package.json"
  },
  "files": [
    "dist/**/*",
    "LICENSE",
    "README.md",
    "CHANGELOG.md",
    "AI_INSTRUCTIONS.md",
    "AI_EXAMPLES.md",
    "AI_QUICK_REFERENCE.md"
  ],
  "keywords": [
    "web-components",
    "lit",
    "react",
    "ai-prototyping",
    "ai-coding-agents",
    "ui-variations",
    "design-variations",
    "multiple-versions",
    "compare-components",
    "a-b-testing",
    "prototyping",
    "design-iteration",
    "component-variations",
    "ai-generated-ui",
    "design-systems",
    "design-tools",
    "frontend",
    "component-library",
    "interactive-prototypes",
    "claude-code",
    "chatgpt-ui",
    "ai-development",
    "typescript",
    "vanilla-extract",
    "zustand",
    "custom-elements"
  ],
  "scripts": {
    "dev": "vite",
    "clean": "rm -rf dist",
    "prebuild": "pnpm run clean",
    "build": "vite build && tsc --project tsconfig.build.json",
    "prebuild:dist": "pnpm run clean",
    "build:dist": "vite build && tsc --project tsconfig.build.json && cp dist/index.d.ts dist/core.d.ts && cp dist/react/index.d.ts dist/react.d.ts",
    "build.watch": "vite build --watch",
    "preview": "vite preview",
    "test": "vitest",
    "test.watch": "vitest --watch",
    "test.ui": "vitest --ui",
    "test.coverage": "vitest --coverage",
    "test.run": "vitest run",
    "lint": "tsc --noEmit",
    "prepack": "pnpm run build:dist",
    "prepublishOnly": "pnpm run build:dist && pnpm run test.run"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "lit": "^3.3.1",
    "zustand": "^5.0.7"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "devDependencies": {
    "@open-wc/testing-helpers": "^3.0.1",
    "@testing-library/jest-dom": "^6.7.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.10",
    "@vanilla-extract/css": "^1.17.4",
    "@vanilla-extract/vite-plugin": "^5.1.1",
    "@vitest/ui": "^3.2.4",
    "happy-dom": "^18.0.1",
    "jsdom": "^26.1.0",
    "react": "^19.1.1",
    "terser": "^5.43.1",
    "typescript": "~5.8.3",
    "vite": "^7.1.2",
    "vitest": "^3.2.4"
  }
}
